#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xpy/Coding/cmcqa/my-docs/node_modules/.pnpm/vuepress@2.0.0-rc.26_@vuepress+bundler-vite@2.0.0-rc.26_@types+node@25.0.3_sass-embedded@1.97.1_sass@1.97.1__vue@3.5.26/node_modules/vuepress/bin/node_modules:/Users/xpy/Coding/cmcqa/my-docs/node_modules/.pnpm/vuepress@2.0.0-rc.26_@vuepress+bundler-vite@2.0.0-rc.26_@types+node@25.0.3_sass-embedded@1.97.1_sass@1.97.1__vue@3.5.26/node_modules/vuepress/node_modules:/Users/xpy/Coding/cmcqa/my-docs/node_modules/.pnpm/vuepress@2.0.0-rc.26_@vuepress+bundler-vite@2.0.0-rc.26_@types+node@25.0.3_sass-embedded@1.97.1_sass@1.97.1__vue@3.5.26/node_modules:/Users/xpy/Coding/cmcqa/my-docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/xpy/Coding/cmcqa/my-docs/node_modules/.pnpm/vuepress@2.0.0-rc.26_@vuepress+bundler-vite@2.0.0-rc.26_@types+node@25.0.3_sass-embedded@1.97.1_sass@1.97.1__vue@3.5.26/node_modules/vuepress/bin/node_modules:/Users/xpy/Coding/cmcqa/my-docs/node_modules/.pnpm/vuepress@2.0.0-rc.26_@vuepress+bundler-vite@2.0.0-rc.26_@types+node@25.0.3_sass-embedded@1.97.1_sass@1.97.1__vue@3.5.26/node_modules/vuepress/node_modules:/Users/xpy/Coding/cmcqa/my-docs/node_modules/.pnpm/vuepress@2.0.0-rc.26_@vuepress+bundler-vite@2.0.0-rc.26_@types+node@25.0.3_sass-embedded@1.97.1_sass@1.97.1__vue@3.5.26/node_modules:/Users/xpy/Coding/cmcqa/my-docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vuepress-webpack.js" "$@"
else
  exec node  "$basedir/../../bin/vuepress-webpack.js" "$@"
fi
